        Return "ноль рублей"
    End If

    Dim intPortion As Integer = Int(number)
    Dim decPortion As Integer = CInt((number - intPortion) * 100)
    Dim words As String = ""

    If intPortion &gt; 0 Then
        Dim intWords As String = ""
        Dim part As Integer

        ' Тысячи
        part = intPortion \ 1000
        If part &gt; 0 Then
            If part &lt; 20 Then
                intWords &amp;= " " &amp; unitsMap(part)
            Else
                intWords &amp;= " " &amp; tensMap(part \ 10)
                If (part Mod 10) &gt; 0 Then
                    intWords &amp;= " " &amp; unitsMap(part Mod 10)
                End If
            End If
            intWords &amp;= " " &amp; thousandsMap(part Mod 10) &amp; thousandsSuffixes(part Mod 10)
        End If
        intPortion = intPortion Mod 1000

        ' Сотни
        part = intPortion \ 100
        If part &gt; 0 Then
            intWords &amp;= " " &amp; hundredsMap(part)
        End If
        intPortion = intPortion Mod 100

        ' Десятки и единицы
        If intPortion &lt; 20 Then
            intWords &amp;= " " &amp; unitsMap(intPortion)
        Else
            intWords &amp;= " " &amp; tensMap(intPortion \ 10)
            If (intPortion Mod 10) &gt; 0 Then
                intWords &amp;= " " &amp; unitsMap(intPortion Mod 10)
            End If
        End If

        words = intWords.Trim() &amp; " рублей"
    End If

    If decPortion &gt; 0 Then
        words &amp;= " " &amp; decPortion.ToString("00") &amp; " копеек"
    End If

    Return words.Trim()
End Function</Code>
